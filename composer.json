{
    "name": "wastewise/capstone",
    "description": "WasteWise Management System",
    "require": {
        "php": ">=7.4",
        "ext-gd": "*",
        "ext-zip": "*",
        "phpmailer/phpmailer": "^6.9",
        "phpoffice/phpspreadsheet": "^1.29.0",
        "tecnickcom/tcpdf": "^6.7.0",
        "vlucas/phpdotenv": "^5.6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "includes/",
            "App\\Mail\\": "includes/mail/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
